
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetOrderList</title>
</head>
<body>
    <div>訂單ID</div>
    <div>@Html.TextBox("txtOrderId")</div>
    <div> 
        <table>
            <tr>
                <td>客戶代號</td>
                <td>客戶名稱</td>
                <td>負責員工</td>
            </tr>
            @foreach (var item in (List<eSalex.Models.Order>)ViewBag.Data)
            {
                <tr>
                    <td>@item.CustId</td>
                    <td>@item.CustName</td>
                    <td>@item.EmpName</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
